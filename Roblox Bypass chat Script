local _0x1234 = {
    A = "ğŸ…",
    B = "ğŸ…‘",
    C = "ğŸ…’",
    D = "ğŸ…“",
    E = "ğŸ…”",
    F = "ğŸ…•",
    G = "ğŸ…–",
    H = "ğŸ…—",
    I = "ğŸ…˜",
    J = "ğŸ…™",
    K = "ğŸ…š",
    L = "ğŸ…›",
    M = "ğŸ…œ",
    N = "ğŸ…",
    O = "ğŸ…",
    P = "ğŸ…Ÿ",
    Q = "ğŸ… ",
    R = "ğŸ…¡",
    S = "ğŸ…¢",
    T = "ğŸ…£",
    U = "ğŸ…¤",
    V = "ğŸ…¥",
    W = "ğŸ…¦",
    X = "ğŸ…§",
    Y = "ğŸ…¨",
    Z = "ğŸ…©"
}

local function _0x5678(_0x9abc)
    local _0xdef0 = ""
    for _0x1 = 1, #_0x9abc do
        local _0x2345 = _0x9abc:sub(_0x1, _0x1):upper()
        local _0x6789 = _0x1234[_0x2345] or _0x2345
        _0xdef0 = _0xdef0 .. _0x6789
    end
    return _0xdef0
end

local function _0x9876()
    local _0xabcd = game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar

    local function _0x5432()
        local _0xdef1 = _0xabcd.Text
        _0xabcd.Text = _0x5678(_0xdef1)
        _0xabcd.CursorPosition = #_0xabcd.Text + 1
    end

    _0xabcd:GetPropertyChangedSignal("Text"):Connect(_0x5432)
end

local function _0x8765()
    local _0x4321 = game:GetService("TextChatService")
    local _0x6789 = game:GetService("Players").LocalPlayer

    _0x4321.OnIncomingMessage = function(_0x9999)
        if _0x9999.TextSource and _0x9999.TextSource.UserId == _0x6789.UserId then
            local _0xaaa0 = _0x5678(_0x9999.Text)
            _0x9999.Text = _0xaaa0
        end
    end
end

local _0x2346 = game:GetService("TextChatService")

if _0x2346.ChatVersion then
    _0x8765()
else
    _0x9876()
end

