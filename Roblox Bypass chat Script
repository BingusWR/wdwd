local _0x4567 = game:GetService("Players").LocalPlayer.PlayerGui.Chat.Frame.ChatBarParentFrame.Frame.BoxFrame.Frame.ChatBar

local _0x7890 = false

_0x4567:GetPropertyChangedSignal("Text"):Connect(function()
    if not _0x7890 then
        _0x7890 = true
        
        local _0x0123 = _0x4567.Text
        local _0x4568 = ""
        
        for _0x5678 = 1, #_0x0123 do
            local _0x7891 = _0x0123:sub(_0x5678, _0x5678):upper()
            local _0x8901 = _0x6789[_0x7891] or _0x7891
            _0x4568 = _0x4568 .. _0x8901
        end
        
        _0x4567.Text = _0x4568
        _0x4567.CursorPosition = #_0x4568 + 1
        
        _0x7890 = false
    end
end)
